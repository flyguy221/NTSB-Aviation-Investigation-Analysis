   "source": [
    "import re\n",
    "import html\n",
    "\n",
    "# Reads the FDF file content\n",
    "def read_fdf_file(file_path):\n",
    "    with open(file_path, 'r', encoding='latin-1') as f:\n",
    "        return f.read()\n",
    "\n",
    "# Function to extract and clean content from <span> tags\n",
    "def clean_span_contents(content):\n",
    "    # Extracts content within <span> tags\n",
    "    span_pattern = re.compile(r'<span[^>]*>(.*?)</span>', re.DOTALL)\n",
    "    span_contents = span_pattern.findall(content)\n",
    "    \n",
    "    # Clean extracted content\n",
    "    cleaned_contents = [html.unescape(span_content.replace(\"\\\\r\", \"\\n\").replace(\"\\\\n\", \"\\n\")) for span_content in span_contents]\n",
    "    cleaned_contents = [re.sub(r'\\\\(.)', r'\\1', content) for content in cleaned_contents]  # Handles escaped characters\n",
    "    \n",
    "    return ' '.join(cleaned_contents)\n",
    "\n",
    "# Processes each object in the FDF file\n",
    "def process_fdf_objects(content):\n",
    "    obj_pattern = re.compile(r'(\\d+ 0 obj)(.*?)(endobj)', re.DOTALL)\n",
    "    objects = obj_pattern.findall(content)\n",
    "    \n",
    "    for obj_id, obj_content, _ in objects:\n",
    "        # Use the clean_span_contents function to extract and clean the text\n",
    "        cleaned_text = clean_span_contents(obj_content)\n",
    "        \n",
    "        if cleaned_text:\n",
    "            print(f\"Object ID: {obj_id}\\nCleaned Text: {cleaned_text}\\n\")\n",
    "        else:\n",
    "            print(f\"Object ID: {obj_id}\\nNo text found.\\n\")\n",
    "\n",
    "# Update the file path to your FDF file's actual location\n",
    "file_path = '/Users/jeremyfeagan/Desktop/test.fdf'  # Make sure to update this path\n",
    "fdf_content = read_fdf_file(file_path)\n",
    "process_fdf_objects(fdf_content)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "46af2c1d",
   "metadata": {},
   "outputs": [],
   "source": [
    "import re\n",
    "import html\n",
    "\n",
    "file_path = '/Users/jeremyfeagan/Desktop/test.fdf'\n",
    "\n",
    "# Reads the FDF file content\n",
    "def read_fdf_file(file_path):\n",
    "    with open(file_path, 'r', encoding='latin-1') as f:\n",
    "        return f.read()\n",
    "\n",
    "# Processes each object in the FDF file\n",
    "def process_fdf_objects(content):\n",
    "    # Regex to find object blocks\n",
    "    obj_pattern = re.compile(r'(\\d+ 0 obj)(.*?)(endobj)', re.DOTALL)\n",
    "    objects = obj_pattern.findall(content)\n",
    "\n",
    "    for obj_id, obj_content, _ in objects:\n",
    "        print(f\"Object ID: {obj_id}\")\n",
    "\n",
    "        # Extracts content within <span> tags if present, otherwise fall back to /Contents(...) pattern\n",
    "        span_pattern = re.compile(r'<span[^>]*>(.*?)</span>', re.DOTALL)\n",
    "        contents_match = span_pattern.search(obj_content) or re.search(r'/Contents\\((.*?)\\)', obj_content, re.DOTALL)\n",
    "\n",
    "        if contents_match:\n",
    "            raw_contents = contents_match.group(1)\n",
    "            # Handle line breaks, HTML entities, and escaped characters\n",
    "            cleaned_contents = html.unescape(raw_contents.replace(\"\\\\r\", \"\\n\").replace(\"\\\\n\", \"\\n\"))\n",
    "            cleaned_contents = re.sub(r'\\\\(.)', r'\\1', cleaned_contents)\n",
    "            print(f\"Cleaned Contents: {cleaned_contents}\")\n",
    "        else:\n",
    "            print(\"No contents found.\")\n",
    "\n",
    "        # Other fields extraction for completeness\n",
    "        nm_match = re.search(r'/NM\\(([^)]+)\\)', obj_content)\n",
    "        subj_match = re.search(r'/Subj\\(([^)]+)\\)', obj_content)\n",
    "        rect_match = re.search(r'/Rect\\[(.*?)\\]', obj_content)\n",
    "        irt_match = re.search(r'/IRT\\(([^)]+)\\)', obj_content)\n",
    "\n",
    "        if nm_match: print(f\"NM: {nm_match.group(1)}\")\n",
    "        if subj_match: print(f\"Subj: {subj_match.group(1)}\")\n",
    "        if rect_match: print(f\"Rect: {rect_match.group(1)}\")\n",
    "        if irt_match: print(f\"IRT: {irt_match.group(1)}\")\n",
    "        print(\"------\")\n",
    "\n",
    "# Update the file path to your FDF file's actual location\n",
    "file_path = '/Users/jeremyfeagan/Desktop/test.fdf'  # Make sure to update this path\n",
    "fdf_content = read_fdf_file(file_path)\n",
    "process_fdf_objects(fdf_content)\n"
   ]
  },
