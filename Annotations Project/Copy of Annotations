   "source": [
    "import re\n",
    "import html\n",
    "\n",
    "file_path = '/Users/jeremyfeagan/Desktop/test.fdf'\n",
    "output_file_path = '/Users/jeremyfeagan/Desktop/test292.txt'\n",
    "\n",
    "def read_fdf_file(file_path):\n",
    "    with open(file_path, 'r', encoding='latin-1') as f:\n",
    "        return f.read()\n",
    "\n",
    "def extract_annotations(fdf_content):\n",
    "    annotations = []\n",
    "    \n",
    "    # Use regex to find annotations\n",
    "    pattern = re.compile(r'/NM\\s*\\(([^)]+)\\).*?/Subj\\s*\\(([^)]+)\\).*?/Contents\\s*\\(([^)]+)\\).*?(?:/IRT\\s*\\(([^)]+)\\))?', re.DOTALL)\n",
    "    \n",
    "    for match in pattern.finditer(fdf_content):\n",
    "        nm, subj, contents, irt = match.groups()\n",
    "        contents = html.unescape(contents)  # Decode HTML entities\n",
    "        annotation_type = \"Comment\" if \"Comment\" in subj else \"Highlight\"\n",
    "        annotations.append({\"NM\": nm, \"Type\": annotation_type, \"Content\": contents, \"IRT\": irt})\n",
    "    \n",
    "    return annotations\n",
    "\n",
    "def build_hierarchy(annotations):\n",
    "    items = {item[\"NM\"]: item for item in annotations}\n",
    "    hierarchy = []\n",
    "\n",
    "    for item in annotations:\n",
    "        item[\"Children\"] = []\n",
    "        if item[\"IRT\"]:\n",
    "            parent = items.get(item[\"IRT\"])\n",
    "            if parent:\n",
    "                parent[\"Children\"].append(item)\n",
    "        else:\n",
    "            hierarchy.append(item)\n",
    "    return hierarchy\n",
    "\n",
    "def format_hierarchy(items, level=0):\n",
    "    result = []\n",
    "    for item in items:\n",
    "        indent = \"  \" * level\n",
    "        result.append(f\"{indent}{item['Type']}: {item['Content']}\")\n",
    "        if item[\"Children\"]:\n",
    "            result.extend(format_hierarchy(item[\"Children\"], level + 1))\n",
    "    return result\n",
    "\n",
    "def read_fdf_file(file_path):\n",
    "    with open(file_path, 'r', encoding='latin-1') as f:\n",
    "        return f.read()\n",
    "\n",
    "def write_output_to_file(formatted_hierarchy, output_file_path):\n",
    "    with open(output_file_path, 'w', encoding='utf-8') as f:\n",
    "        for line in formatted_hierarchy:\n",
    "            f.write(f\"{line}\\n\")\n",
    "\n",
    "# Main logic\n",
    "fdf_content = read_fdf_file(file_path)\n",
    "raw_annotations = extract_annotations(fdf_content)\n",
    "hierarchy = build_hierarchy(raw_annotations)\n",
    "formatted_hierarchy = format_hierarchy(hierarchy)\n",
    "write_output_to_file(formatted_hierarchy, output_file_path)\n",
    "\n",
    "print(f\"Output saved to {output_file_path}\")\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "22c2c862",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "'/Users/jeremyfeagan/Desktop/test292.txt'"
      ]
     },
     "execution_count": 9,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "import re\n",
    "import html\n",
    "from collections import defaultdict\n",
    "\n",
    "# First, let's read the content of the uploaded FDF file\n",
    "file_path = '/Users/jeremyfeagan/Desktop/Adobe FDF.txt'\n",
    "\n",
    "def read_fdf_file(file_path):\n",
    "    with open(file_path, 'r', encoding='latin-1') as f:\n",
    "        return f.read()\n",
    "\n",
    "fdf_content = read_fdf_file(file_path)\n",
    "\n",
    "def extract_annotations(fdf_content):\n",
    "    annotations = []\n",
    "    \n",
    "    # Use regex to find annotations, including a capture for /Subtype to differentiate highlights and comments\n",
    "    pattern = re.compile(r'/NM\\s*\\(([^)]+)\\).*?/Subtype\\s*/([^/]+).*?/Subj\\s*\\(([^)]+)\\).*?/Contents\\s*\\(([^)]+)\\).*?(?:/IRT\\s*\\(([^)]+)\\))?', re.DOTALL)\n",
    "    \n",
    "    for match in pattern.finditer(fdf_content):\n",
    "        nm, subtype, subj, contents, irt = match.groups()\n",
    "        contents = html.unescape(contents)  # Decode HTML entities\n",
    "        # Adjusting to include Subtype for identifying highlights and comments more accurately\n",
    "        annotation_type = \"Highlight\" if subtype == \"Highlight\" else \"Comment\"\n",
    "        annotations.append({\"NM\": nm, \"Type\": annotation_type, \"Content\": contents, \"IRT\": irt, \"Subj\": subj})\n",
    "    \n",
    "    return annotations\n",
    "\n",
    "raw_annotations = extract_annotations(fdf_content)\n",
    "\n",
    "# Now, to organize comments with their respective parent (if any) and maintain hierarchical structure\n",
    "def organize_annotations(annotations):\n",
    "    organized = defaultdict(lambda: {\"content\": \"\", \"children\": []})\n",
    "    for annotation in annotations:\n",
    "        nm = annotation[\"NM\"]\n",
    "        irt = annotation[\"IRT\"]\n",
    "        content = annotation[\"Content\"]\n",
    "        organized[nm][\"content\"] = annotation[\"Type\"] + \": \" + content\n",
    "        if irt:  # If it has a parent\n",
    "            organized[irt][\"children\"].append(nm)\n",
    "    \n",
    "    return organized\n",
    "\n",
    "def format_annotations(organized, nm, depth=0):\n",
    "    output = []\n",
    "    annotation = organized[nm]\n",
    "    indent = \"    \" * depth  # Increase indentation for nested comments\n",
    "    output.append(f\"{indent}•\\t{annotation['content']}\")\n",
    "    for child_nm in annotation[\"children\"]:\n",
    "        output.extend(format_annotations(organized, child_nm, depth + 1))\n",
    "    return output\n",
    "\n",
    "organized_annotations = organize_annotations(raw_annotations)\n",
    "\n",
    "# Assuming the first annotation does not have a parent and can act as a root for demonstration\n",
    "if raw_annotations:\n",
    "    root_nm = raw_annotations[0][\"NM\"]\n",
    "    formatted_annotations = format_annotations(organized_annotations, root_nm)\n",
    "\n",
    "    # Let's write the formatted annotations to a text file\n",
    "    output_file_path = '/Users/jeremyfeagan/Desktop/test292.txt'\n",
    "    with open(output_file_path, 'w') as f:\n",
    "        for line in formatted_annotations:\n",
    "            f.write(line + '\\n')\n",
    "\n",
    "output_file_path"
   ]
  },
