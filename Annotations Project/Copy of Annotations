   "source": [
    "import re\n",
    "import html\n",
    "\n",
    "def read_fdf_file(file_path):\n",
    "    with open(file_path, 'r', encoding='latin-1') as f:\n",
    "        return f.read()\n",
    "\n",
    "def process_fdf_content(content):\n",
    "    # Step 1: Extract Annotations\n",
    "    pattern = re.compile(r'(\\d+ 0 obj)(.*?)(endobj)', re.DOTALL)\n",
    "    matches = pattern.findall(content)\n",
    "\n",
    "    for match in matches:\n",
    "        obj_id, obj_content, _ = match\n",
    "        print(f\"Object ID: {obj_id}\")  # Debugging print\n",
    "\n",
    "        # Extract fields\n",
    "        nm = re.search(r'/NM\\(([^)]+)\\)', obj_content)\n",
    "        subj = re.search(r'/Subj\\(([^)]+)\\)', obj_content)\n",
    "        rect = re.search(r'/Rect\\[(.*?)\\]', obj_content)\n",
    "        irt = re.search(r'/IRT\\(([^)]+)\\)', obj_content)\n",
    "\n",
    "        # Adjusted regex to extract text specifically within <span> tags\n",
    "        span_pattern = re.compile(r'<span[^>]*>(.*?)</span>', re.DOTALL)\n",
    "        span_matches = span_pattern.findall(obj_content)\n",
    "\n",
    "        for content in span_matches:\n",
    "            # Cleaning HTML entities\n",
    "            cleaned_content = html.unescape(content)\n",
    "            print(f\"Cleaned Content: {cleaned_content}\")\n",
    "\n",
    "        if nm and subj:\n",
    "            print(f\"NM: {nm.group(1)}, Subj: {subj.group(1)}\")\n",
    "            if rect:\n",
    "                print(f\"Rect: {rect.group(1)}\")\n",
    "            if irt:\n",
    "                print(f\"IRT: {irt.group(1)}\")\n",
    "        print(\"------\")\n",
    "\n",
    "file_path = '/Users/jeremyfeagan/Desktop/adobe fdf.txt'  # Update this path to your actual FDF file path\n",
    "content = read_fdf_file(file_path)\n",
    "process_fdf_content(content)\n"
   ]
