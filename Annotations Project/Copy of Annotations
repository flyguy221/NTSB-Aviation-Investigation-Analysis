 "cells": [
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "864f657b",
   "metadata": {},
   "outputs": [],
   "source": [
    "import re\n",
    "\n",
    "# Function to read the FDF file content\n",
    "def read_fdf_content(file_path):\n",
    "    with open(file_path, 'r', encoding='latin-1') as f:\n",
    "        return f.read()\n",
    "\n",
    "# Function to parse the content and extract annotations\n",
    "def parse_annotations(content):\n",
    "    pattern = re.compile(r'(\\d+ 0 obj).*?(endobj)', re.DOTALL)\n",
    "    annotations = []\n",
    "\n",
    "    for match in pattern.findall(content):\n",
    "        obj_id, obj_content = match\n",
    "        nm_match = re.search(r'/NM\\(([^)]+)\\)', obj_content)\n",
    "        contents_match = re.search(r'/Contents\\(([^)]+)\\)', obj_content)\n",
    "        subj_match = re.search(r'/Subj\\(([^)]+)\\)', obj_content)\n",
    "        rect_match = re.search(r'/Rect\\[(.*?)\\]', obj_content)\n",
    "        irt_match = re.search(r'/IRT\\(([^)]+)\\)', obj_content)\n",
    "        \n",
    "        if nm_match and contents_match and subj_match:\n",
    "            annotations.append({\n",
    "                'id': nm_match.group(1),\n",
    "                'contents': contents_match.group(1).replace('\\\\r', '\\n').replace(r'\\(', '(').replace(r'\\)', ')'),\n",
    "                'subj': subj_match.group(1),\n",
    "                'rect': rect_match.group(1) if rect_match else '',\n",
    "                'irt': irt_match.group(1) if irt_match else '',\n",
    "            })\n",
    "\n",
    "    return annotations\n",
    "\n",
    "# Function to organize annotations into quotes and their comments\n",
    "def organize_annotations(annotations):\n",
    "    quotes = {}\n",
    "    comments = {}\n",
    "\n",
    "    for annot in annotations:\n",
    "        if annot['subj'] == 'Highlight':\n",
    "            quotes[annot['id']] = {**annot, 'comments': []}\n",
    "        else:\n",
    "            comments[annot['id']] = annot\n",
    "    \n",
    "    for cid, comment in comments.items():\n",
    "        if comment['irt'] in quotes:\n",
    "            quotes[comment['irt']]['comments'].append(comment)\n",
    "        elif comment['irt'] in comments:  # Nested comments\n",
    "            parent_comment = comments[comment['irt']]\n",
    "            if 'nested_comments' not in parent_comment:\n",
    "                parent_comment['nested_comments'] = []\n",
    "            parent_comment['nested_comments'].append(comment)\n",
    "    \n",
    "    return quotes\n",
    "\n",
    "# Function to display quotes and their comments\n",
    "def display_quotes(quotes):\n",
    "    for qid, quote in quotes.items():\n",
    "        print(f'Quote from text: “{quote[\"contents\"]}”')\n",
    "        if quote['comments']:\n",
    "            print('\\tComments:')\n",
    "            for comment in quote['comments']:\n",
    "                print(f'\\t- {comment[\"contents\"]}')\n",
    "                for nested_comment in comment.get('nested_comments', []):\n",
    "                    print(f'\\t\\t- {nested_comment[\"contents\"]}')\n",
    "\n",
    "# Main execution\n",
    "file_path = '/Users/jeremyfeagan/Desktop/test.fdf'  # Update with the correct file path\n",
    "content = read_fdf_content(file_path)\n",
    "annotations = parse_annotations(content)\n",
    "quotes = organize_annotations(annotations)\n",
    "display_quotes(quotes)"
   ]
  },
